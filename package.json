{
  "name": "map-project-backend",
  "version": "1.0.0",
  "description": "This is the Map Project backend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "contributors": [
    "santoshnepal608@gmail.com"
  ],
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": " rm -rf dist && tsc   ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" && echo \"Success: no linter errors\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "debug": "cross-env NODE_ENV=DEV nodemon --inspect=0.0.0.0:9229 -e ts,tsx --exec node -r ts-node/register ./src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs -d typeorm-config.mg.ts",
    "m:create": "sh -c 'yarn typeorm  migration:generate src/migrations/$0'",
    "m:up": "yarn typeorm migration:run",
    "m:down": "yarn typeorm migration:revert",
    "m:show": "yarn typeorm migration:show"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.5",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.0",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-license-header": "^0.6.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.19.1",
    "node": ">=20.0.0"
  }
}
